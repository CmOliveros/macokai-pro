<idea-plugin>
  <id>macokai-pro</id>
  <name>Macokai Pro Theme</name>
  <vendor url="https://subtheme.dev">Cody Oliveros</vendor>
  <version>1.8.1</version>

  <depends>com.intellij.modules.lang</depends>
  <idea-version since-build="191.0"/>

  <change-notes><![CDATA[]]>
  </change-notes>

  <description>
  <![CDATA[
  A shameless fork of Subtheme's Monokai Pro theme, modified to match colors with macOS.
  <br/><br/>
  This repo is a mess.  I thoroughly welcome somebody who knows anything about Java/Jetbrains themes to help fix it at this <a href="https://github.com/CmOliveros/macokai-pro">GitHub repo</a>.
  <br/><br/>
  Screenshots were taken with <a href="https://plugins.jetbrains.com/plugin/10044-atom-material-icons">Atom Material Icons</a>.
  <br/><br/>
  <img src="https://i.imgur.com/m0IOJHA.png" border="0" width="360"/><br/>

  ]]>
  </description>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="1bf66303-7318-44c3-8fae-6c6f0eb3fd53" path="/classic.theme.json"/>
    <themeProvider id="96c210a3-cc3b-47c2-ba0f-6de6f50c18a3" path="/machine.theme.json"/>
    <themeProvider id="342c21d9-7947-4ea7-8291-d4fd4e96fa36" path="/octagon.theme.json"/>
    <themeProvider id="76cc8066-d21c-4e4c-8583-1b3f22e90e8f" path="/ristretto.theme.json"/>
    <themeProvider id="ddd7798e-8bd6-4aa2-8345-a08190090321" path="/spectrum.theme.json"/>
    <themeProvider id="b8abb5dc-b0c9-4d20-97f7-ec5cde31cfd7" path="/default.theme.json"/>

    <bundledColorScheme path="/schemes/classic-no-italics"/>
    <bundledColorScheme path="/schemes/machine-no-italics"/>
    <bundledColorScheme path="/schemes/octagon-no-italics"/>
    <bundledColorScheme path="/schemes/ristretto-no-italics"/>
    <bundledColorScheme path="/schemes/spectrum-no-italics"/>
    <bundledColorScheme path="/schemes/default-no-italics"/>
  </extensions>
</idea-plugin>
